{
  "name": "zleslie-ssh",
  "version": "1.6.0",
  "author": "zleslie",
  "summary": "Puppet ssh management module",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-ssh.git",
  "project_page": "https://github.com/xaque208/puppet-ssh",
  "issues_url": "https://github.com/xaque208/puppet-ssh/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "5.9",
        "5.8"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10.3",
        "10.2"
      ]
    },
    {
      "operatingsystem": "Darwin",
      "operatingsystemrelease": [
        "15.5.0"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Archlinux",
      "operatingsystemrelease": [
        "4.7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 8.0.0"
    }
  ],
  "description": "A Puppet module to configure OpenSSH servers",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.0"
    },
    {
      "name": "puppetlabs/sshkey_core",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    }
  ]
}
